---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% unless page.url contains '/assets/' or page.url contains '/files/' %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
      {% if page.date %}
        <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>monthly</changefreq>
      <priority>{% if page.url == '/' %}1.0{% else %}0.8{% endif %}</priority>
    </url>
    {% endunless %}
  {% endfor %}
  
  {% for exam in site.exams %}
    <url>
      <loc>{{ site.url }}{{ exam.url }}</loc>
      <changefreq>yearly</changefreq>
      <priority>0.7</priority>
    </url>
  {% endfor %}
  
  {% for participant in site.participants %}
    <url>
      <loc>{{ site.url }}{{ participant.url }}</loc>
      <changefreq>yearly</changefreq>
      <priority>0.6</priority>
    </url>
  {% endfor %}
</urlset>
